<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ffdd1eff" />
            <gradient android:angle="-90" android:startColor="#ff621764" android:endColor="#ff621764"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#5e7974" />
            <solid android:color="#58857e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="180dip" />
            <stroke android:width="1dip" android:color="#ff621764" />
            <gradient android:angle="-180" android:startColor="#E37FD7" android:endColor="#A3DD1EFF" />
        </shape>
    </item>
</selector>